nt = (\n    <Fragment>\n      {CartItems}\n      <div className={style.total}>\n        <span>Total</span>\n        <span>{Cartctx.totalAmount}</span>\n      </div>\n      {!isOrder && modelAction}\n      {isOrder && (\n        <Checkout onConfirm={cardSubmitHandler} onCancel={props.onClick} />\n      )}\n    </Fragment>\n  );\n\n  const cardSubmitted = (\n    <div className={style.actions}>\n      <p>Sent data</p>\n      <button className={style[\"button--alt\"]} onClick={props.onClick}>\n        Close\n      </button>\n      </div>\n  );\n  return (\n    <Model onClick={props.onClick}>\n      {!isSubmitting && !haveSubmitted && cardModelContent}\n      {isSubmitting && !haveSubmitted && <p>Sending....</p>}\n      {!isSubmitting && haveSubmitted && cardSubmitted}\n    </Model>\n  );\n};\n\nexport default Cart;\n","/home/manikandan/Desktop/react/01-starting-project-foodapp/src/components/UI/Model.js",[],"/home/manikandan/Desktop/react/01-starting-project-foodapp/src/store/CartProvider.js",[],"/home/manikandan/Desktop/react/01-starting-project-foodapp/src/store/cart-contex.js",[],"/home/manikandan/Desktop/react/01-starting-project-foodapp/src/components/Cart/CartItem.js",[],"/home/manikandan/Desktop/react/01-starting-project-foodapp/src/components/Cart/Checkout.js",[],{"ruleId":"78","severity":1,"message":"79","line":47,"column":6,"nodeType":"80","endLine":47,"endColumn":8,"suggestions":"81"},{"ruleId":"82","severity":1,"message":"83","line":49,"column":11,"nodeType":"84","messageId":"85","endLine":49,"endColumn":14},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'loadDataHandler'. Either include it or remove the dependency array.","ArrayExpression",["86"],"no-unused-vars","'res' is assigned a value but never used.","Identifier","unusedVar",{"desc":"87","fix":"88"},"Update the dependencies array to be: [loadDataHandler]",{"range":"89","text":"90"},[1125,1127],"[loadDataHandler]"]